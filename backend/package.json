    "ethers": "^5.6.4",
    "graphql": "^16.3.0",
    "nexus": "^1.3.0",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/node": "^17.0.24",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
