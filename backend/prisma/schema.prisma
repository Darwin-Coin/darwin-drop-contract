generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  wallet    String   @unique
}


model AirDrop {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  creator   String
  startTime DateTime 
  endTime   DateTime
  status    String  
}


enum Role {
  USER
  CREATOR
}
